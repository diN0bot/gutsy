.environment
  h1 Environments
  ul
    for environ in project.environments
      li #{environ.type}
        ul
          li #{environ.platforms} #{environ.regions}
          if environ.url.auth
            li <a href=#{environ.url.auth.username}:#{environ.url.auth.username}@#{environ.url.url} >#{environ.url.url}</a>
          else
            a(href='#{environ.url.url}') #{environ.url.url}
